# Display the grade by inputing 5 marks
a=[]
for i in range(0,5):
    y=eval(input("Enter the mark:"))
    a.append(y)
x=sum(a)/5
if (x>=90):
        print("Grade is A")
elif (x>=80 and x<90):
        print("Grade is B")
elif (x>=70 and x<80):
        print("Grade is C")
elif (x>=60 and x<70):
        print("Grade is D")
elif (x>=50 and x<60):
        print("Grade is E")
else:
        print("Grade is F")
